<Window x:Class="MachineLearning.WindowInsertKnn"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MachineLearning"
        mc:Ignorable="d"
        Title="Insert point" Height="300" Width="300" Loaded="Window_Loaded">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="90*" />
            <RowDefinition Height="15*" />
        </Grid.RowDefinitions>
        <StackPanel Name="skpContainer" Grid.Row="0">
        
        </StackPanel>
        <StackPanel HorizontalAlignment="Center" Grid.Row="1" Orientation="Horizontal">
            <Button Name="btnOK" Content="OK" Padding="10 2" Click="btnOK_Click" Margin="5"/>
            <Button Name="btnCancel" Content="Cancel" Padding="10 2" Click="btnCancel_Click" Margin="5"/>
        </StackPanel>
    </Grid>
</Window>
